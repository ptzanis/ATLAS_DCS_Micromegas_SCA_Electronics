#uses "FwDevMode.ctl"
MMG_SIDE_A_MMG_SIDE_A_STATUS_MMG_SIDE_A_FWDM_install()
{
   string devdp;

   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A/STATUS_MMG_SIDE_A_FWDM");
   if(!globalExists("FwDevMode_CurrDUActions"))
   {
      addGlobal("FwDevMode_CurrDUActions",DYN_STRING_VAR);
      fwDU_getAllActions("FwDevMode",FwDevMode_CurrDUActions);
   }
   FwDevMode_initialize("MMG_SIDE_A", devdp);
   fwDU_setDefaultParameters("MMG_SIDE_A", devdp);
   if(dpConnect("cbMMG_SIDE_A_MMG_SIDE_A_STATUS_MMG_SIDE_A_FWDM_valueChanged",
      devdp+".disabled:_online.._value"
     ) == -1 )
      DebugN("Bad dpConnect state");
}

MMG_SIDE_A_MMG_SIDE_A_STATUS_MMG_SIDE_A_FWDM_uninstall(string dp, int running)
{
   string devdp;

DebugN("uninstalling MMG_SIDE_A MMG_SIDE_A/STATUS_MMG_SIDE_A_FWDM");
   if(running) return;
   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A/STATUS_MMG_SIDE_A_FWDM");
   if(dpDisconnect("cbMMG_SIDE_A_MMG_SIDE_A_STATUS_MMG_SIDE_A_FWDM_valueChanged",
      devdp+".disabled:_online.._value"
     ) == -1 )
      DebugN("Bad dpDisconnect state");
}

cbMMG_SIDE_A_MMG_SIDE_A_STATUS_MMG_SIDE_A_FWDM_valueChanged(
      string dp1, dyn_string x1   )
{
   string state;
   string devdp;

   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A/STATUS_MMG_SIDE_A_FWDM");
   FwDevMode_valueChanged("MMG_SIDE_A", devdp,
      x1, state );
   fwDU_setState("MMG_SIDE_A", devdp, state);
}

MMG_SIDE_A_MMG_SIDE_A_FWDM_install()
{
   string devdp;

   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A_FWDM");
   if(!globalExists("FwDevMode_CurrDUActions"))
   {
      addGlobal("FwDevMode_CurrDUActions",DYN_STRING_VAR);
      fwDU_getAllActions("FwDevMode",FwDevMode_CurrDUActions);
   }
   FwDevMode_initialize("MMG_SIDE_A", devdp);
   fwDU_setDefaultParameters("MMG_SIDE_A", devdp);
   if(dpConnect("cbMMG_SIDE_A_MMG_SIDE_A_FWDM_valueChanged",
      devdp+".disabled:_online.._value"
     ) == -1 )
      DebugN("Bad dpConnect state");
}

MMG_SIDE_A_MMG_SIDE_A_FWDM_uninstall(string dp, int running)
{
   string devdp;

DebugN("uninstalling MMG_SIDE_A MMG_SIDE_A_FWDM");
   if(running) return;
   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A_FWDM");
   if(dpDisconnect("cbMMG_SIDE_A_MMG_SIDE_A_FWDM_valueChanged",
      devdp+".disabled:_online.._value"
     ) == -1 )
      DebugN("Bad dpDisconnect state");
}

cbMMG_SIDE_A_MMG_SIDE_A_FWDM_valueChanged(
      string dp1, dyn_string x1   )
{
   string state;
   string devdp;

   devdp = ""+fwDU_getPhysicalName("MMG_SIDE_A_FWDM");
   FwDevMode_valueChanged("MMG_SIDE_A", devdp,
      x1, state );
   fwDU_setState("MMG_SIDE_A", devdp, state);
}

