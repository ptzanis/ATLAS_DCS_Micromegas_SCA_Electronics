class: $FWPART_MMG_ELTX_CHAMBER_FwDevMode_CLASS
    state: READY
        action: Disable(Device)
            remove &VAL_OF_Device from $FWPART_MMG_ELTX_CHAMBER_FWSETSTATES
            remove &VAL_OF_Device from $FWPART_MMG_ELTX_CHAMBER_FWSETACTIONS
            move_to READY
        action: Enable(Device)
            insert &VAL_OF_Device in $FWPART_MMG_ELTX_CHAMBER_FWSETSTATES
            insert &VAL_OF_Device in $FWPART_MMG_ELTX_CHAMBER_FWSETACTIONS
            move_to READY

object: $FWPART_MMG_ELTX_CHAMBER_FWDM is_of_class $FWPART_MMG_ELTX_CHAMBER_FwDevMode_CLASS

