"main()
{
     addSymbol(myModuleName(),myPanelName(),\"/objects/fsmAtlas/fwFsmAtlas_cu.pnl\",\"sectorStateStatus_\"+$obj,
              makeDynString(\"$domain:\"+domain,\"$obj:\"+$obj,\"$label:\"+\"\"),
              300, 20, 0, 1.0, 1.0);


  string layerObj;

  int x=10;
  int y=80;

  int xBoard=0;
  int yBoard=0;


 for(int i=1; i<=8 ;i++)
  {
   layerObj=$obj+\"_LAYER_\"+i;

    addSymbol(myModuleName(),myPanelName(),\"/objects/layerStatusState.pnl\",\"layerStateStatus_\"+layerObj,
          makeDynString(\"$domain:\"+domain,\"$obj:\"+layerObj,\"$label:\"+\"\"),
          x, y, 0, 0.5, 0.5);

   x=x+105;


  dyn_string allChildren;
  dyn_int allFlags;

  int boardNum;

  fwFsmAtlasGetTreeNodes(allChildren, allFlags,layerObj);

//   int xInitial=20+110*(i-1);
  int xInitial=x-60;
  int yInitial=500;

   int countBoard=1;

  for(int board_i=2; board_i<=dynlen(allChildren); board_i++)
  {

   boardNum=getChamberBoard(allChildren[board_i]);

   if(boardNum==1)
   { xBoard=xInitial; yBoard=yInitial;}
   if(boardNum==2)
   {
    xBoard=xInitial+30; yBoard=yInitial;
   }
   if(boardNum==3)
   {
    xBoard=xInitial; yBoard=yInitial-20;
    if(i==2 || i==6 || i==4 || i==8)
    {yInitial=yBoard-20;}
   }
   if(boardNum==4)
   {

    if(i==4 || i==8)
    {
      xBoard=xInitial+30;
      yInitial=yBoard+100;
    }
    else
    {
    xBoard=xInitial+30;
    yBoard=yInitial-20;//43
    }

//      countBoard=1;
    yInitial=yBoard-20;
   }

    addSymbol(myModuleName(),myPanelName(),\"/mmg_eltx_tools/boards/boardRectangle.pnl\",\"StatusState_\"+allChildren[board_i] ,
    makeDynString(\"$domain:\"+domain,\"$obj:\"+allChildren[board_i],\"$label:\"+\"\"),
    xBoard,yBoard,0,0.6,0.6);

     countBoard++;



  }





  }





