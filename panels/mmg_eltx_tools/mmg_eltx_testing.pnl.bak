  string obj;
  string side=radioSideSelection.text(radioSideSelection.number());
  string sector=comboSectorSelection.text();
  string layer=comboLayerSelection.text();
  string board=boardList.text();

  dyn_string layerBoards=returnBoardsOfLayer(layer);


  if(board!=\"\")
  {
     obj=\"MMG_SIDE_\"+side+\"_SECTOR_\"+sector+\"_LAYER_\"+layer+\"_BOARD_\"+board;
     dpSet(eltxProjectName(obj)+getDetector(obj)+\"_Side\"+getSide(obj)+\".Sector\"+getSector(obj)+\".Z\"+getChamberZaxis(obj)+\".R\"+getChamberRaxis(obj)+\".Layer\"+getChamberLayer(obj)+\".PCB\"+getChamberPCB(obj)+\".Board\"+getChamberBoard(obj)+\".state.power\",(int)this.text());
     DebugN(side,sector,layer,board,getBoardChannel(obj),\"power\",(int)this.text());
  }
  else
  {
     for(int i=1; i<=dynlen(layerBoards); i++)
     {
     obj=\"MMG_SIDE_\"+side+\"_SECTOR_\"+sector+\"_LAYER_\"+layer+\"_BOARD_\"+layerBoards[i];
     dpSet(eltxProjectName(obj)+getDetector(obj)+\"_Side\"+getSide(obj)+\".Sector\"+getSector(obj)+\".Z\"+getChamberZaxis(obj)+\".R\"+getChamberRaxis(obj)+\".Layer\"+getChamberLayer(obj)+\".PCB\"+getChamberPCB(obj)+\".Board\"+getChamberBoard(obj)+\".state.power\",(int)this.text());
     DebugN(side,sector,layer,layerBoards[i],getBoardChannel(obj),\"power\",(int)this.text());

     }
   }

 }

