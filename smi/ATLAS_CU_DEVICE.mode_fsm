class: $FWPART_ATLAS_CU_DEVICE_FwDevMode_CLASS
    state: READY
        action: Disable(Device)
            remove &VAL_OF_Device from $FWPART_ATLAS_CU_DEVICE_FWSETSTATES
            remove &VAL_OF_Device from $FWPART_ATLAS_CU_DEVICE_FWSETACTIONS
            move_to READY
        action: Enable(Device)
            insert &VAL_OF_Device in $FWPART_ATLAS_CU_DEVICE_FWSETSTATES
            insert &VAL_OF_Device in $FWPART_ATLAS_CU_DEVICE_FWSETACTIONS
            move_to READY

object: $FWPART_ATLAS_CU_DEVICE_FWDM is_of_class $FWPART_ATLAS_CU_DEVICE_FwDevMode_CLASS

